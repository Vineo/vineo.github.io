    function handleSkinLoaded()
    {
        renderSkin(this);
    }
    function handleImageError()
    {
        var replacement = new Image();
        replacement.src = defaultImageSrc;
        replacement.setAttribute('class', this.getAttribute('class'));
        this.parentNode.replaceChild(replacement, this);
    }
    function renderSkin(skinImage)
    {
        var canvas = document.createElement('canvas');
        canvas.width = 37.2 * scale;
        canvas.height = 32.8 * scale;
        canvas.setAttribute('class', skinImage.getAttribute('class'));
        var context = canvas.getContext("2d");
        context.imageSmoothingEnabled = false;
        context.mozImageSmoothingEnabled = false;
        var s = scale;
        
        /* front */
        context.drawImage(skinImage, 8,  8,  8, 8,  4.4 * s, 0.4 * s,  8 * s, 8 * s);
        context.drawImage(skinImage, 20, 20, 8, 12, 4.4 * s, 8.4 * s, 8 * s, 12 * s);
        context.drawImage(skinImage, 4,  20, 4, 12, 4.4 * s, 20.4 * s, 4 * s, 12 * s);
        context.drawImage(skinImage, 4,  20, 1, 12, 11.4* s, 20.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 5,  20, 1, 12, 10.4* s, 20.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 6,  20, 1, 12, 9.4* s, 20.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 7,  20, 1, 12, 8.4* s, 20.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 44, 20, 4, 12, 0.4 * s, 8.4 * s, 4 * s, 12 * s);
        context.drawImage(skinImage, 44, 20, 1, 12, 15.4* s, 8.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 45, 20, 1, 12, 14.4* s, 8.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 46, 20, 1, 12, 13.4* s, 8.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 47, 20, 1, 12, 12.4* s, 8.4 * s, 1 * s, 12 * s);
        
        /* front Accessores */
        context.drawImage(skinImage, 4,  36, 4, 12, 4 * s, 20* s, 4.8 * s, 12.8 * s);
        context.drawImage(skinImage, 4,  52, 4, 12, 8 * s, 20* s, 4.8 * s, 12.8 * s);
        context.drawImage(skinImage, 44, 36, 4, 12, 0 * s, 8 * s, 4.8 * s, 12.8 * s);
        context.drawImage(skinImage, 52, 52, 4, 12, 12* s, 8 * s, 4.8 * s, 12.8 * s);
        context.drawImage(skinImage, 20, 36, 8, 12, 4 * s, 8 * s, 8.8 * s, 12.8 * s);
        context.drawImage(skinImage, 40, 8,  8, 8,  4 * s, 0* s,  8.8 * s, 8.8 * s );
   
        /* Back */
        context.drawImage(skinImage, 24, 8,  8, 8,  24.4 * s, 0.4 * s,  8 * s, 8 * s);
        context.drawImage(skinImage, 32, 20, 8, 12, 24.4 * s, 8.4 * s, 8 * s, 12 * s);
        context.drawImage(skinImage, 12, 20, 4, 12, 28.4 * s, 20.4 * s, 4 * s, 12 * s);
        context.drawImage(skinImage, 12, 20, 1, 12, 27.4 * s, 20.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 13, 20, 1, 12, 26.4 * s, 20.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 14, 20, 1, 12, 25.4 * s, 20.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 15, 20, 1, 12, 24.4 * s, 20.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 52, 20, 1, 12, 23.4 * s, 8.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 53, 20, 1, 12, 22.4 * s, 8.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 54, 20, 1, 12, 21.4 * s, 8.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 55, 20, 1, 12, 20.4 * s, 8.4 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 52, 20, 4, 12, 32.4 * s, 8.4 * s, 4 * s, 12 * s);
        
        /* Back Accessores */
        context.drawImage(skinImage, 12, 36, 4, 12, 27.9 * s, 20* s, 4.8 * s, 12.8 * s);
        context.drawImage(skinImage, 12, 52, 4, 12, 24 * s, 20* s, 4.8 * s, 12.8 * s); 
        context.drawImage(skinImage, 60, 52, 4, 12, 20 * s, 8 * s, 4.8 * s, 12.8 * s);
        context.drawImage(skinImage, 52, 36, 4, 12, 32 * s, 8 * s, 5.1 * s, 12.8 * s);
        context.drawImage(skinImage, 32, 36, 8, 12, 24 * s, 8 * s, 8.8 * s, 12.8 * s);
        context.drawImage(skinImage, 56, 8,  8, 8,  24 * s, 0* s,  8.8 * s, 8.8 * s);
        skinImage.parentNode.replaceChild(canvas, skinImage);
    }

    function renderSkin2denry(skinImage)
    {
        var canvas = document.createElement('canvas');
        canvas.width = 58 * scale;
        canvas.height = 32 * scale;
        canvas.setAttribute('class', skinImage.getAttribute('class'));
        var context = canvas.getContext("2d");
        context.imageSmoothingEnabled = false;
        context.mozImageSmoothingEnabled = false;
        var s = scale;
        context.drawImage(skinImage, 8, 8, 8, 8, 4 * s, 0 * s, 8 * s, 8 * s);
context.drawImage(skinImage, 40,  8,  8,  8,  4*s,  0*s,  8*s, 8*s);
        context.drawImage(skinImage, 20, 20, 8, 12, 4 * s, 8 * s, 8 * s, 12 * s);
        context.drawImage(skinImage, 4, 20, 4, 12, 4 * s, 20 * s, 4 * s, 12 * s);
        context.drawImage(skinImage, 4, 20, 1, 12, 11 * s, 20 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 5, 20, 1, 12, 10 * s, 20 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 6, 20, 1, 12, 9 * s, 20 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 7, 20, 1, 12, 8 * s, 20 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 44, 20, 4, 12, 0 * s, 8 * s, 4 * s, 12 * s);
        context.drawImage(skinImage, 44, 20, 1, 12, 15 * s, 8 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 45, 20, 1, 12, 14 * s, 8 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 46, 20, 1, 12, 13 * s, 8 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 47, 20, 1, 12, 12 * s, 8 * s, 1 * s, 12 * s);
        context.drawImage(skinImage, 24, 8, 8, 8, 24 * s, 0 * s, 8 * s, 8 * s);
context.drawImage(skinImage, 56,  8,  8, 8,  24*s,  0*s,  8*s, 8*s);
        context.drawImage(skinImage, 32, 20, 8, 12, 24*s,  8*s,  8*s, 12*s);  
        context.drawImage(skinImage, 12,  20, 4, 12, 28*s,  20*s, 4*s, 12*s);  
        context.drawImage(skinImage, 12,  20, 1, 12, 27*s,  20*s, 1*s, 12*s); 
        context.drawImage(skinImage, 13,  20, 1, 12, 26*s,  20*s, 1*s, 12*s); 
        context.drawImage(skinImage, 14,  20, 1, 12, 25*s,  20*s, 1*s, 12*s); 
        context.drawImage(skinImage, 15,  20, 1, 12, 24*s,  20*s, 1*s, 12*s); 
        context.drawImage(skinImage, 52,  20, 1, 12, 23*s,  8*s, 1*s, 12*s); 
        context.drawImage(skinImage, 53,  20, 1, 12, 22*s,  8*s, 1*s, 12*s); 
        context.drawImage(skinImage, 54,  20, 1, 12, 21*s,  8*s, 1*s, 12*s); 
        context.drawImage(skinImage, 55,  20, 1, 12, 20*s,  8*s, 1*s, 12*s); 
        context.drawImage(skinImage, 52, 20, 4, 12, 32*s, 8*s,  4*s, 12*s);  
        context.drawImage(skinImage, 16,  8,  8, 8,  41*s,  0*s,  8*s, 8*s);  
        context.drawImage(skinImage, 0,  20, 1, 12, 46*s,  20*s, 1*s, 12*s); 
        context.drawImage(skinImage, 1,  20, 1, 12, 45*s,  20*s, 1*s, 12*s); 
        context.drawImage(skinImage, 2,  20, 1, 12, 44*s,  20*s, 1*s, 12*s); 
        context.drawImage(skinImage, 3,  20, 1, 12, 43*s,  20*s, 1*s, 12*s); 
        context.drawImage(skinImage, 40,  20, 1, 12, 46*s,  8*s, 1*s, 12*s); 
        context.drawImage(skinImage, 41,  20, 1, 12, 45*s,  8*s, 1*s, 12*s); 
        context.drawImage(skinImage, 42,  20, 1, 12, 44*s,  8*s, 1*s, 12*s); 
        context.drawImage(skinImage, 43,  20, 1, 12, 43*s,  8*s, 1*s, 12*s); 
        context.drawImage(skinImage, 0,  8,  8, 8,  50*s,  0*s,  8*s, 8*s);   
        context.drawImage(skinImage, 0,  20, 4, 12, 52*s,  20*s, 4*s, 12*s);  
        context.drawImage(skinImage, 40, 20, 4, 12, 52*s,  8*s,  4*s, 12*s);  
        skinImage.parentNode.replaceChild(canvas, skinImage);
    }
    
    
    
function renderSkin1d(skinImage)
    {
        var canvas = document.createElement('canvas');
        canvas.width = 36 * scale;
        canvas.height = 32 * scale;
        canvas.setAttribute('class', skinImage.getAttribute('class'));
        var context = canvas.getContext("2d");
        context.imageSmoothingEnabled = false;
        context.mozImageSmoothingEnabled = false;     
        

        
        var s = scale;
        var h = 2;

        /*Зад */
        context.drawImage(skinImage, 24*h,  8*h,  8*h, 8*h,  24*s,  0*s,  8*s, 8*s);
        context.drawImage(skinImage, 56*h,  8*h,  8*h, 8*h,  24*s,  0*s,  8*s, 8*s);
        context.drawImage(skinImage, 32*h, 20*h, 8*h, 12*h, 24*s,  8*s,  8*s, 12*s);
        /* п нога */
        context.drawImage(skinImage, 12*h,  20*h, 4*h, 12*h, 28*s,  20*s, 4*s, 12*s);
        /* п рука */
        context.drawImage(skinImage, 52*h, 20*h, 4*h, 12*h, 32*s, 8*s,  4*s, 12*s);    
        /* Перед */
        context.drawImage(skinImage, 8*h,  8*h,  8*h, 8*h,  4*s,  0*s,  8*s, 8*s);
        context.drawImage(skinImage, 40*h,  8*h,  8*h, 8*h,  4*s,  0*s,  8*s, 8*s);
        context.drawImage(skinImage, 20*h, 20*h, 8*h, 12*h, 4*s,  8*s,  8*s, 12*s); 
        /*л нога */
        context.drawImage(skinImage, 4*h,  20*h, 4*h, 12*h, 4*s,  20*s, 4*s, 12*s); 
        /* л рука */
        context.drawImage(skinImage, 44*h, 20*h, 4*h, 12*h, 0 * s, 8 * s, 4 * s, 12 * s);
        /* Отраженные части*/
        context.scale(-1, 1);
        context.translate(-skinImage.width, 0);
        /*перед п нога */
        context.drawImage(skinImage, 4*h,  20*h, 4*h, 12*h, 4*s,  20*s, 4*s, 12*s);
        /*перед п рука */
        context.drawImage(skinImage, 44*h, 20*h, 4*h, 12*h, 0*s, 8 * s, 4 * s, 12 * s)
        /*зад л нога */
        context.drawImage(skinImage, 12*h,  20*h, 4*h, 12*h, -12*s,  20*s, 4*s, 12*s);
        /*зад л рука */
        context.drawImage(skinImage, 52*h, 20*h, 4*h, 12*h, -8*s, 8*s,  4*s, 12*s);

        
           
        skinImage.parentNode.replaceChild(canvas, skinImage);
    }
    
    
    
    
    
    function renderSkin2d(skinImage)
    {
        var canvas = document.createElement('canvas');
        canvas.width = 36 * scale;
        canvas.height = 32 * scale;
        canvas.setAttribute('class', skinImage.getAttribute('class'));
        var context = canvas.getContext("2d");
        context.imageSmoothingEnabled = false;
        context.mozImageSmoothingEnabled = false;     
        

        
        var s = scale;
        var h = 4;
        /*Зад */
        context.drawImage(skinImage, 24*h,  8*h,  8*h, 8*h,  24*s,  0*s,  8*s, 8*s);
        context.drawImage(skinImage, 56*h,  8*h,  8*h, 8*h,  24*s,  0*s,  8*s, 8*s);
        context.drawImage(skinImage, 32*h, 20*h, 8*h, 12*h, 24*s,  8*s,  8*s, 12*s);
        /* п нога */
        context.drawImage(skinImage, 12*h,  20*h, 4*h, 12*h, 28*s,  20*s, 4*s, 12*s);
        /* п рука */
        context.drawImage(skinImage, 52*h, 20*h, 4*h, 12*h, 32*s, 8*s,  4*s, 12*s);    
        /* Перед */
        context.drawImage(skinImage, 8*h,  8*h,  8*h, 8*h,  4*s,  0*s,  8*s, 8*s);
        context.drawImage(skinImage, 40*h,  8*h,  8*h, 8*h,  4*s,  0*s,  8*s, 8*s);
        context.drawImage(skinImage, 20*h, 20*h, 8*h, 12*h, 4*s,  8*s,  8*s, 12*s); 
        /*л нога */
        context.drawImage(skinImage, 4*h,  20*h, 4*h, 12*h, 4*s,  20*s, 4*s, 12*s); 
        /* л рука */
        context.drawImage(skinImage, 44*h, 20*h, 4*h, 12*h, 0 * s, 8 * s, 4 * s, 12 * s);
        /* Отраженные части*/
        context.scale(-1, 1);
        context.translate(-skinImage.width, 0);
        /*перед п нога */
        context.drawImage(skinImage, 4*h,  20*h, 4*h, 12*h, 20*s,  20*s, 4*s, 12*s);
        /*перед п рука */
        context.drawImage(skinImage, 44*h, 20*h, 4*h, 12*h, 16*s, 8 * s, 4 * s, 12 * s)
        /*зад л нога */
        context.drawImage(skinImage, 12*h,  20*h, 4*h, 12*h, 4*s,  20*s, 4*s, 12*s);
        /*зад л рука */
        context.drawImage(skinImage, 52*h, 20*h, 4*h, 12*h, 8*s, 8*s,  4*s, 12*s);

        skinImage.parentNode.replaceChild(canvas, skinImage);
    }
    
    
    
    
    
    
    
    
    function renderSkin3d(skinImage)
    {
        var canvas = document.createElement('canvas');
        canvas.width = 36 * scale;
        canvas.height = 32 * scale;
        canvas.setAttribute('class', skinImage.getAttribute('class'));
        var context = canvas.getContext("2d");
        context.imageSmoothingEnabled = false;
        context.mozImageSmoothingEnabled = false;     
        

        
        var s = scale;
        var h = 8;
         /*Зад */
        context.drawImage(skinImage, 24*h,  8*h,  8*h, 8*h,  24*s,  0*s,  8*s, 8*s);
        context.drawImage(skinImage, 56*h,  8*h,  8*h, 8*h,  24*s,  0*s,  8*s, 8*s);
        context.drawImage(skinImage, 32*h, 20*h, 8*h, 12*h, 24*s,  8*s,  8*s, 12*s);
        /* п нога */
        context.drawImage(skinImage, 12*h,  20*h, 4*h, 12*h, 28*s,  20*s, 4*s, 12*s);
        /* п рука */
        context.drawImage(skinImage, 52*h, 20*h, 4*h, 12*h, 32*s, 8*s,  4*s, 12*s);    
        /* Перед */
        context.drawImage(skinImage, 8*h,  8*h,  8*h, 8*h,  4*s,  0*s,  8*s, 8*s);
        context.drawImage(skinImage, 40*h,  8*h,  8*h, 8*h,  4*s,  0*s,  8*s, 8*s);
        context.drawImage(skinImage, 20*h, 20*h, 8*h, 12*h, 4*s,  8*s,  8*s, 12*s); 
        /*л нога */
        context.drawImage(skinImage, 4*h,  20*h, 4*h, 12*h, 4*s,  20*s, 4*s, 12*s); 
        /* л рука */
        context.drawImage(skinImage, 44*h, 20*h, 4*h, 12*h, 0 * s, 8 * s, 4 * s, 12 * s);
        /* Отраженные части*/
        context.scale(-1, 1);
        context.translate(-skinImage.width, 0);
        /*перед п нога */
        context.drawImage(skinImage, 4*h,  20*h, 4*h, 12*h, 52*s,  20*s, 4*s, 12*s);
        /*перед п рука */
        context.drawImage(skinImage, 44*h, 20*h, 4*h, 12*h, 48*s, 8 * s, 4 * s, 12 * s)
        /*зад л нога */
        context.drawImage(skinImage, 12*h,  20*h, 4*h, 12*h, 36*s,  20*s, 4*s, 12*s);
        /*зад л рука */
        context.drawImage(skinImage, 52*h, 20*h, 4*h, 12*h, 40*s, 8*s,  4*s, 12*s);
           
        skinImage.parentNode.replaceChild(canvas, skinImage);
    }
    
    
    
    
    
