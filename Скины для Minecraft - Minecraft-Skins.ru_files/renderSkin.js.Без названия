    function getElementsByClassName(className, tag, elm)
    {
        var testClass = new RegExp("(^|\\s)" + className + "(\\s|$)");
        var tag = tag || "*";
        var elm = elm || document;
        var elements = (tag == "*" && elm.all) ? elm.all : elm.getElementsByTagName(tag);
        var returnElements = [];
        var current;
        var length = elements.length;
        for (var i = 0; i < length; i++)
        {
            current = elements[i];
            if (testClass.test(current.className)) {
                returnElements.push(current);
            }
        }
        return returnElements;
    }
    function supportsCanvas()
    {
        canvas_compatible = false;
        try {
            canvas_compatible = !!(document.createElement('canvas').getContext('2d'));
        }
        catch (e) {
            canvas_compatible = !!(document.createElement('canvas').getContext);
        }
        return canvas_compatible;
    }
    renderMCSkins('skin', 3.86);
    renderMCSkins('canvas-2d', 8);
    renderMCSkins('skins-repost-site', 4);
    renderMCSkins('randers-skins-random', 2);
    renderMCSkins1d('canvas-128', 8);
    renderMCSkins2d('canvas-256', 8);
    renderMCSkins3d('canvas-512', 8);
    renderMCSkins4d('canvas-1024', 8);
    renderMCSkins5d('canvas-2048', 8);
    renderMCSkins1dc('cut-canvas-128', 6.25);
    renderMCSkins2dc('cut-canvas-256', 6.25);
    renderMCSkins3dc('cut-canvas-512', 6.25);
    renderMCSkins4dc('cut-canvas-1024', 6.25);
    renderMCSkins5dc('cut-canvas-2048', 6.25); 
}(window.onload=body_load);